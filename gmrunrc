# Set window geometry
Width = 200
Top = 350
Left = 250

# You can make aliases withing gmrun, for example:

#type Ctrl+Enter
Terminal = urxvt

TermExec = urxvt -e
URL_http = chromium-bin %u
URL_https = chromium-bin %u

#type google:<keyword>
URL_google = chromium-bin 'http://www.google.com.ph/search?q=%s'

#type mail://
URL_mail = chromium-bin https://mail.google.com/

#type [command] Ctrl+Enter
URL_top = ${TermExec} '%s'
URL_mc = ${TermExec} '%s'

